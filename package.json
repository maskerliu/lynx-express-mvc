{
  "name": "lynx-express-mvc",
  "version": "1.0.35",
  "description": "a express mvc support middleware",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "yarn build:lib-esm && yarn build:extract-api && yarn build:umd",
    "build:umd": "node --experimental-json-modules --import tsx ./builder.ts",
    "build:lib-esm": "tsc -p tsconfig.json --declarationDir ./dist/typings-temp -m es6 --outDir dist/lib-esm",
    "build:extract-api": "api-extractor run ",
    "dev": "node --experimental-specifier-resolution=node --import tsx ./test/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maskerliu/lynx-express-mvc.git"
  },
  "keywords": [
    "express",
    "MVC",
    "Typescript",
    "webpack"
  ],
  "author": "chrisliu",
  "license": "ISC",
  "homepage": "https://github.com/maskerliu/lynx-express-mvc#readme",
  "bugs": {
    "url": "https://github.com/maskerliu/lynx-express-mvc/issues"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/lib-esm/index.js",
      "require": "./dist/lib-umd/index.cjs"
    }
  },
  "typings": "dist/typings/index.d.ts",
  "dependencies": {
    "express": "^4.21.0",
    "express-fileupload": "^1.5.1",
    "json-bigint": "^1.0.0",
    "pouchdb-adapter-leveldb": "^9.0.0",
    "pouchdb-find": "^9.0.0",
    "pouchdb-node": "^9.0.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/plugin-proposal-decorators": "^7.25.7",
    "@babel/plugin-syntax-decorators": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@microsoft/api-extractor": "^7.47.9",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-fileupload": "^1.5.1",
    "@types/node": "^22.7.4",
    "@types/webpack": "^5.28.5",
    "babel-loader": "^9.2.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "del": "^7.1.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  }
}
